<div>
  <h2><%= link_to(tweet.title, tweet) %></h2>
  <p>By: <%= tweet.user.email %></p>
  <p><%= tweet.body %></p>
  <p>At: <%= tweet.location %></p>
  <p><%= image_tag(tweet.photo.url(:full)) %></p>
  <% if user_signed_in? %>
    <p> <%= edit_tweet_link(tweet) %></p>
    <p><%= delete_tweet_link(tweet) %></p>
  <% end %>
</div>
